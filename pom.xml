        <dependency>
            <groupId>org.webjars.npm</groupId>
            <artifactId>jwt-decode</artifactId>
            <version>4.0.0</version>
        </dependency>
        <dependency>
            <groupId>org.webjars.npm</groupId>
            <artifactId>react-bootstrap</artifactId>
            <version>2.10.1</version>
        </dependency>
        <dependency>
            <groupId>org.webjars.npm</groupId>
            <artifactId>react-chartjs-2</artifactId>
            <version>5.2.0</version>
        </dependency>
        <dependency>
            <groupId>com.github.japgolly.react</groupId>
