        <dependency>
            <groupId>com.github.japgolly.react</groupId>
            <artifactId>dom</artifactId>
            <version>18.2.0</version>
        </dependency>
        <dependency>
            <groupId>org.webjars.npm</groupId>
            <artifactId>@emotion/react</artifactId>
            <version>11.11.3</version>
        </dependency>
        <dependency>
            <groupId>org.webjars.npm</groupId>
            <artifactId>@emotion/styled</artifactId>
            <version>11.11.0</version>
        </dependency>
        <dependency>
            <groupId>org.webjars.npm</groupId>
            <artifactId>@fontsource/roboto</artifactId>
